<?xml version="1.0" encoding="UTF-8"?>

<launch>

	<arg name="rviz_config" default="$(find open3d_slam)/rviz/icp_benchmark.rviz" />
	<arg name="launch_rviz" default="true" />
	<arg name="cloud_filename" default="$(find open3d_slam)/data/icp_benchmark.pcd"/>
	

	<node name="icp_bechmark_node" pkg="open3d_slam"
		type="icp_benchmark_node" output="screen" launch-prefix="">	
		<param name="cloud_filename" value="$(arg cloud_filename)"/>	
	</node>

	<node name="rviz_icp" pkg="rviz" type="rviz"
		args="-d $(arg rviz_config)"
		if="$(arg launch_rviz)">
	</node>

</launch>